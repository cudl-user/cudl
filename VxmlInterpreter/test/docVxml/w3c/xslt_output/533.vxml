<?xml version="1.0" encoding="UTF-8"?><vxml xmlns="http://www.w3.org/2001/vxml" xmlns:conf="http://www.w3.org/2002/vxml-conformance" version="2.0">
  <var name="count" expr="0"/>
  <catch><prompt>fail</prompt><log>failure expression: <value expr="'catch-all caught ' + _event"/></log><prompt><value expr="'catch-all caught ' + _event"/></prompt><exit/></catch>
  <form id="first">
    <block>
      <assign name="count" expr="1"/>
      <goto next="#third"/>
    </block>
  </form>
  <form id="second">
    <block>
      <prompt>fail</prompt><log>failure expression: <value expr="'went to the second form'"/></log><prompt><value expr="'went to the second form'"/></prompt><exit/>
    </block>
  </form>
  <form id="third">
    <block>
      <if cond="count==1">
        <prompt>pass</prompt><exit/>
      </if>
      <prompt>fail</prompt><log>failure expression: <value expr="'Count is wrong, should be 1 but is (' + count + ') instead"/></log><prompt><value expr="'Count is wrong, should be 1 but is (' + count + ') instead"/></prompt><exit/>
    </block>
  </form>
</vxml>