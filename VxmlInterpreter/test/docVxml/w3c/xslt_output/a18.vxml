<?xml version="1.0" encoding="UTF-8"?><vxml xmlns="http://www.w3.org/2001/vxml" xmlns:conf="http://www.w3.org/2002/vxml-conformance" version="2.0">

  <catch><prompt>fail</prompt><log>failure expression: <value expr="'catch-all caught ' + _event"/></log><prompt><value expr="'catch-all caught ' + _event"/></prompt><exit/></catch>
  <menu>
    <prompt count="1">
      Say 'Chicago'.
    </prompt><prompt count="2"> 
      Say 'Chicago' again.      
    </prompt><prompt count="3">       
      Say 'Chicago' one more time.
    </prompt>
    <choice next="#form1">
  Chicago
    </choice> 
    <choice next="#form2">
  San Francisco
    </choice> 
  </menu>
  
  <form id="noop">
    <block>
      <prompt>fail</prompt><log>failure reason: Form not referenced</log><prompt>Form not referenced</prompt><exit/>
    </block>
  </form>

  <form id="form1">
    <block>
      <prompt>pass</prompt><exit/>
    </block>
  </form>

  <form id="form2">
    <block>
      <prompt>fail</prompt><log>failure reason: Transition to unspecified form</log><prompt>Transition to unspecified form</prompt><exit/>
    </block>
  </form>


</vxml>