<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml" xmlns:conf="http://www.w3.org/2002/vxml-conformance">
	<catch>
		<conf:fail expr="'catch-all caught ' + _event"/>
	</catch>
	<form id="first">
		<block>
			<conf:pass/>
		</block>
	</form>
	<form id="second">
		<block>
			<conf:fail reason="interpreter must begin execution of the first form"/>
		</block>
	</form>
</vxml>
